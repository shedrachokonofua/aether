  - model_name: "aether/gpt-oss:20b"
    litellm_params:
      model: "openai/gpt-oss:20b"
      litellm_credential_name: ollama_openai_credential
  - model_name: "aether/glm-4.7-flash:q8_0"
    litellm_params:
      model: "openai/glm-4.7-flash:q8_0"
      litellm_credential_name: ollama_openai_credential

  # OpenAI models
  - model_name: "openai/gpt-5.2-pro"
    litellm_params:
      model: "openai/gpt-5.2-pro"
      litellm_credential_name: openai_credential
  - model_name: "openai/gpt-5.2"
    litellm_params:
      model: "openai/gpt-5.2"
      litellm_credential_name: openai_credential

  # Anthropic models
  - model_name: "anthropic/claude-opus-4.5"
    litellm_params:
      model: "anthropic/claude-opus-4-5"
      litellm_credential_name: anthropic_credential
  - model_name: "anthropic/claude-sonnet-4-5"
    litellm_params:
      model: "anthropic/claude-sonnet-4-5"
      litellm_credential_name: anthropic_credential

  # Reranker via TEI
  - model_name: "aether/bge-reranker-large"
    litellm_params:
      model: huggingface/BAAI/bge-reranker-large
      api_base: http://{{ vm.gpu_workstation.ip }}:{{ vm.gpu_workstation.ports.reranker }}

  # OpenRouter models
  - model_name: "openrouter/glm-4.6"
    litellm_params:
      model: "openai/z-ai/glm-4.6"
      litellm_credential_name: openrouter_credential
  - model_name: "openrouter/grok-4"
    litellm_params:
      model: "openai/x-ai/grok-4"
      litellm_credential_name: openrouter_credential
  - model_name: "openrouter/gemini-3-pro-preview"
    litellm_params:
      model: "openai/google/gemini-3-pro-preview"
      litellm_credential_name: openrouter_credential

credential_list:
  - credential_name: ollama_credential
    credential_values:
      api_base: https://ollama.home.shdr.ch
      api_key: none
    credential_info:
      description: "Ollama Credential"
  - credential_name: ollama_openai_credential
    credential_values:
      api_base: https://ollama.home.shdr.ch/v1
      api_key: none
    credential_info:
      description: "Ollama OpenAI Compatible Credential"
  - credential_name: openai_credential
    credential_values:
      api_key: os.environ/OPENAI_API_KEY
    credential_info:
      description: "OpenAI"
  - credential_name: anthropic_credential
    credential_values:
      api_key: os.environ/ANTHROPIC_API_KEY
    credential_info:
      description: "Anthropic"
  - credential_name: openrouter_credential
    credential_values:
      api_base: https://openrouter.ai/api/v1
      api_key: os.environ/OPENROUTER_API_KEY
    credential_info:
      description: "OpenRouter"

litellm_settings:
  drop_params: true
  turn_off_message_logging: false
  set_verbose: true
  json_logs: true

general_settings:
  store_prompts_in_spend_logs: true

mcp_servers:
  time:
    url: "http://localhost:8003/mcp"
  firecrawl:
    url: "https://mcp.firecrawl.home.shdr.ch/mcp"
  alpha_vantage:
    url: "https://mcp.alphavantage.co/mcp?apikey={{ secrets.alphavantage_api_key }}"
  coingecko:
    url: "http://localhost:8002/"
  finviz:
    url: "http://localhost:8000/mcp"
  microsandbox:
    url: "http://host.containers.internal:{{ vm.ai_tool_stack.ports.microsandbox }}/mcp"